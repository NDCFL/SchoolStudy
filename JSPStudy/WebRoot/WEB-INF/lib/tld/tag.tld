<?xml version="1.0" encoding="utf-8" ?>
<taglib xmlns="http://java.sun.com/xml/ns/j2ee" 
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
		xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee Web-jsptaglibrary_2_0.xsd"
		version="2.0">
	<description>tag配置</description>
	<tlib-version>1.0</tlib-version>
	<short-name>samples</short-name>
	<uri>/myselftag</uri>
	<tag>
		<description>你好tag页面</description>
		<name>HellowTag</name>
		<tag-class>TagDemo.HellowTag</tag-class>
		<body-content>empty</body-content>
	</tag>
	<tag>
		<description>字体tag页面</description>
		<name>FontTagDemo</name>
		<tag-class>TagDemo.FontTagDemo</tag-class>
		<body-content>jsp</body-content>
			<attribute>
				<name>size</name>
				<required>false</required>
				<rtexprvalue>true</rtexprvalue>
			</attribute>
			<attribute>
				<name>face</name>
				<required>false</required>
				<rtexprvalue>true</rtexprvalue>
			</attribute>
			<attribute>
				<name>color</name>
				<required>false</required>
				<rtexprvalue>true</rtexprvalue>
			</attribute>
	</tag>
	<tag>
		<description>九九乘法表tag页面</description>
		<name>MathTagDemo</name>
		<tag-class>TagDemo.MathTagDemo</tag-class>
		<body-content>jsp</body-content>
			<attribute>
				<name>count</name>
				<required>true</required>
				<rtexprvalue>true</rtexprvalue>
			</attribute>
	</tag>	
	<tag>
		<description>登录tag页面</description>
		<name>LoginTagDemo</name>
		<tag-class>TagDemo.LoginTagDemo</tag-class>
		<body-content>jsp</body-content>
	</tag>
	<tag>
		<description>分页tag页面</description>
		<name>PagingTagDemo</name>
		<tag-class>TagDemo.PagingTagDemo</tag-class>
		<body-content>jsp</body-content>
		<attribute>
				<name>paging</name>
				<required>true</required>
				<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
				<name>list</name>
				<required>true</required>
				<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
				<name>urlString</name>
				<required>true</required>
				<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>
</taglib>
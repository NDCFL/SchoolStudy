package Loopyuju;


public class TT {

	/**
	 * @param args
	 */
	
	public static void main(String[] args) {
		int sum=0;
		int  t=0,num=0;
//		一个整数，它加上100后是一个完全平方数，再加上168又是一个完全平方数，请问该数是多少？
		for(int n=1;n<30;n++){
			System.out.print(n+"的平方="+n*n+"\t");
			t++;
			if (t%5==0) {
				System.out.println();
			}
		}
		
	}

}

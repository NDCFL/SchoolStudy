package T2;

public class Wanshu {

	public static void main(String[] args) {
		int a;
		for (int i = 6; i <=1000; i++) {
			a=0;
			for (int j = 2; j <=i; j++) {
				if (i%j==0) {
					a+=i/j;
				}
			}
			if (a==i) {
				for (int j = i; j >0; j--) {
					if (i%j==0) {
						if (i==j) {
							System.out.print("+"+i/j);
						}else {
							System.out.print(i/j+"+");
						}
					}
					
				}
				System.out.println("="+a);
			}
		}
	}
}

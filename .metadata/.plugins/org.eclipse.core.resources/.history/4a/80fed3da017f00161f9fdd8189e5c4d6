public class Bianhua extends JFrame{

	JPanel p1,p2;
	JButton fang,yuan;
	public Bianhua() {
		super("方圆转换");
		setSize(500, 500);
//		fang = new JButton("变方");
//		yuan = new JButton("变圆");
//		add(fang);

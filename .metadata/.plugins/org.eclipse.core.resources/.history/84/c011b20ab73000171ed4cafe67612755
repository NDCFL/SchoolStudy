 //<![CDATA[
 $(document).ready(function(){
   getResData();
   //获取数据
   function getResData(){
  jQuery.getJSON("/application/visits/visits.jsp?sid=newthu&r="+Math.random(), function(data){
    $("#today").empty();
    $("#today").append(data[0].today);
    $("#total").empty();
    $("#total").append(data[0].total);
    $("#arverage").empty();
    $("#arverage").append(data[0].arverage);     
  });
   };
 });
